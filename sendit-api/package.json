{
  "name": "sendit-api",
  "version": "1.0.0",
  "description": "API endpoints for the sendIT app",
  "main": "dist",
  "scripts": {
    "dev": "set NODE_ENV=development && nodemon --exec babel-node src --presets airbnb",
    "build": "babel src --out-dir dist --presets airbnb",
    "start": "set NODE_ENV=production && node dist/index.js",
    "lint": "./node_modules/.bin/eslint src",
    "test": "set NODE_ENV=test && mocha --compilers js:@babel/register --require @babel/polyfill --recursive src/test --exit",
    "test-watch": "nodemon --exec \"npm test\"",
    "cover": "set NODE_ENV=test && istanbul cover node_modules/mocha/bin/_mocha -- --ui bdd -R spec -t 5000 --compilers js:@babel/register --recursive \"src/test/*.test.js\"",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls"
  },
  "author": "UGOJI Duziem",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/node": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-preset-airbnb": "^3.0.1",
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.6",
    "supertest": "^3.3.0"
  },
  "dependencies": {
    "express": "^4.16.4"
  }
}
