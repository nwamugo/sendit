{
  "name": "sendit-api",
  "version": "1.0.0",
  "description": "API endpoints for the sendIT app",
  "main": "dist",
  "scripts": {
    "dev": "set NODE_ENV=development && nodemon --exec babel-node src --presets airbnb",
    "build": "babel src --out-dir dist --presets airbnb",
    "start": "set NODE_ENV=production && node dist/app.js",
    "lint": "./node_modules/.bin/eslint src",
    "test": "mocha --compilers js:@babel/register --require @babel/polyfill --recursive src/test",
    "test-watch": "nodemon --exec \"npm test\""
  },
  "author": "UGOJI Duziem",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/node": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-preset-airbnb": "^3.0.1",
    "chai": "^4.2.0",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.6",
    "supertest": "^3.3.0"
  },
  "dependencies": {
    "express": "^4.16.4"
  }
}
